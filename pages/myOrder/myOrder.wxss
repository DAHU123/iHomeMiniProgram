/**sureOrder.wxss**/
.me_person{
    display: flex;
    flex-flow:row nowrap;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1rpx solid #f2f2f2;
    background: #fff;
    height: 80rpx;
    padding: 20rpx;
    width: calc(100% - 40rpx);
    position: fixed;
    left: 0;
    top: 0;
}
.me_person text{
    font-size: 30rpx;
    color: #9a9a9a;
}
.me_goBack{
    width: 15rpx;
    height: 28rpx;
    transform:rotateY(180deg);
}
.me_goTo{
    width: 15rpx;
    height: 28rpx;
    margin-left: 30rpx;
}
.me_threeInfo{
    width: 34rpx;
    height: 28rpx;
}
.me_black{
    display: flex;
    flex-flow:row nowrap;
    justify-content: space-around;
    align-items: center;
    height: 100rpx;
    width: 100%;
    background: #000;
    color: #fff;
    font-size: 20rpx;
    position: fixed;
    left: 0;
    top: 120rpx;
    z-index: 3;
}
.me_yiHang{
    display: flex;
    flex-flow:column nowrap;
    justify-content: flex-start;
    align-items: center;
}
.me_yiHang image{
    width: 24rpx;
    height: 24rpx;
    margin-bottom: 10rpx;
}
/*swiper相关*/
.swiper-tab{
    width: 100%;
    border-bottom: 2rpx solid #e7e7e7;
    text-align: center;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 24rpx;
    background: #fff;
    color: #323232;
    display: flex;
    flex-flow: row;
    justify-content: space-between;
}
.swiper-tab-item{
    width: 30%;
    color:#434343;
}
.me_active{
    color:#ff6128;
    border-bottom: 4rpx solid #ff6128;
    margin-bottom: -2rpx;
}
swiper{
    text-align: center;
}
.me_shopTitle{
    height: 70rpx;
    font-size: 24rpx;
    color: #323232;
    padding: 20rpx;
    background: #fff;
    display: flex;
    flex-flow:row nowrap;
    justify-content: flex-start;
    align-items: center;

}
.me_shopTitle view{
    color: #ff6128;
    flex-grow: 1;
    text-align: right;
}
.me_javaImg{
    background: #f8f8f8;
    width: calc(100% - 20rpx);
    height: 125rpx;
    padding: 30rpx 0 30rpx 20rpx;
    overflow: hidden;
}
.me_javaImg>view{
    width: 200%;
    height: 125rpx;
}
.me_goodsImg{
    width: 140rpx;
    height: 125rpx;
    margin-right: 10rpx;
    float: left;
}
.me_goodsCount{
    padding: 0 20rpx;
    background: #fff;
}
.me_goodsCount > view {
    height: 70rpx;
    color: #999;
    font-size: 20rpx;
    line-height: 70rpx;
    border-bottom: 1rpx solid #e6e6e6;
    text-align: right;
}
.me_goodsCount > view > text:nth-child(1){
    display: inline-block;
    margin-right: 10rpx;
}
.me_goodsCount > view > text:nth-child(2){
    font-size: 24rpx;
    color: #ff6128;
}
.me_cancel{
    background: #fff;
    padding: 0 20rpx;
    height: 88rpx;
    border-bottom: 1rpx solid #e6e6e6;
    margin-bottom: 20rpx;
    display: flex;
    flex-flow:row nowrap;
    justify-content: flex-end;
    align-items: center;

}
.me_cancel view{
    display: inline-block;
    height: 60rpx;
    font-size: 28rpx;
    line-height: 60rpx;
    padding: 0 30rpx;
    color: #999;
    margin-right: 20rpx;
    border: 1rpx solid #ccc;
}
.me_cancel view:nth-child(2){
    color: #fff;
    border-color: #ff612a;
    color: #ff612a;
    margin-right: 0;
}
.me_mask{
    position: fixed;
    width: 100%;
    height: 100%;
    background: transparent;
    z-index: 2;
}