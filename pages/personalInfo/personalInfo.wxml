<!--sureOrder.wxml-->
<view class="me_person">
    <image src="{{goBack}}" class="me_goBack" bindtap="goBack"></image>
    <text>个人信息</text>
    <image src="{{threeInfo}}" class="me_threeInfo" bindtap="showBlack"></image>
</view>
<view class="me_mask" hidden="{{hiddenMask}}" bindtap="hideBlack"></view>
<view class="me_black" hidden="{{hiddenBlack}}">
    <view class="me_yiHang">
        <image src="{{car}}"></image>
        <text>购物车</text>
    </view>
    <view class="me_yiHang">
        <image src="{{people}}"></image>
        <text>个人中心</text>
    </view>
</view>
<view style="padding-top: 80rpx;"></view>
<view class="me_hang ">
    <view class="me_inner_view">
        <text>头像:</text>
        <image src="{{userInfo.avatarUrl}}" class="me_userLogo"></image>
    </view>
</view>
<view class="me_hang">
    <view class="me_inner_view me_height">
        <text>昵称:</text>
        <text>{{userInfo.nickName}}</text>
    </view>
</view>
<view class="me_hang me_last">
    <view class="me_inner_view me_height">
        <text>手机号:</text>
        <text>{{userInfo.phone}}</text>
    </view>
</view>
<view class="me_hang me_last">
    <view class="me_inner_view me_height me_ctrl_width">
        <text>收货地址:</text>
        <text>{{userPhone}}收货地址收货地址收货地址</text>
        <image src="{{goTo}}" class="me_goTo"></image>
    </view>
</view>

