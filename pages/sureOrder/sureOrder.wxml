<view class="me_address" bindtap='address'>
    <view>
        <view class="me_first">
            <text>{{userNickName}}</text>
            <text>{{userPhone}}</text>
        </view>
        <view class="me_left">
            <image src="{{infoAdd}}" class="me_infoAdd"></image>
            <text>地址:{{address}}</text>
        </view>
    </view>
    <image src="{{goTo}}" class="me_goTo"></image>
</view>
