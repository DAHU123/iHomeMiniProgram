</view>
<view class="me_hang">
    <view class="me_inner_view me_height">
        <text>运费</text>
        <text style="color: #999;">+12.00</text>
    </view>
</view>
<view class="me_hang me_last">
    <view class="me_inner_view me_height">
        <text>优惠</text>
        <text style="color: #999;">-0.00</text>
    </view>
</view>
<view class="me_hang me_last">
